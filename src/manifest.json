{
  "name": "重定向M.JIKE",
  "description" : "将即刻移动端网页重定向到 Web 版进行访问",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
      "default_icon": "jike.png",
      "default_title": "重定向M.JIKE"
  },
  "homepage_url": "https://github.com/sinyu1012/RedirectMJike",
  "background": {
    "page": "main.html"
},
  "permissions": [
    "webRequest",
    "https://m.okjike.com/*",
    "webRequestBlocking"
  ]
}
